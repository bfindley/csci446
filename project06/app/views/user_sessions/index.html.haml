title "User Sessions"

%table
  %tr
    %th Username
    %th Password
  - for user_session in @user_sessions
    %tr
      %td= h user_session.username
      %td= h user_session.password
      %td= link_to 'Show', user_session
      %td= link_to 'Edit', edit_user_session_path(user_session)
      %td= link_to 'Destroy', user_session, :confirm => 'Are you sure?', :method => :delete

%p= link_to "New User Session", new_user_session_path
